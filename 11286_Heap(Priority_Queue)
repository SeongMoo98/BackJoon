#include<iostream>
#include<queue>
#include<vector>
using namespace std;

//비교함수를 지정해서 절대값 heap 구현
//비교함수가 아니라 Struct 안에 멤버함수를 구현???
struct compare {
	bool operator()(int a, int b) {

		if (abs(a) == abs(b)) {
			return a > b;
		}
		else {
			return abs(a) > abs(b);
		}
	}
};

int N;

int main(void) {


	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	//vector : 큐 정렬을 위한 Container
	//compare 함수를 통해 우선순위 정해주기
	priority_queue<int, vector<int>, compare> pq; 

	cin >> N;

	for (int i = 0; i < N; i++) {
		int x; cin >> x;

		if (x == 0) {
			if (pq.empty()) {
				cout << "0\n";
			}
			else {
				cout << pq.top() << "\n";
				pq.pop();
			}
		}
		else {
			pq.push(x);
		}
	}


	return 0;
}

