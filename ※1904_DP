#include<iostream>
using namespace std;

// 1 : 1
// 2 : 00 11 
// 3 : 001 100 111 
// 4 : 0000 0011 1100 1001 1111
// 5 : 00001 00100 10000 00111 11001 11100 10011 11111

//끝에 1이 오거나(i-1), 끝에 00이 오거나(i-2)

int N;
long long res[1000001] = { 0,1,2, };

int main(void) {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	cin >> N;

	for (int i = 3; i <= N; i++) {
		//res[i] = res[i - 1] + res[i - 2];

		//여기서 나누어주지 않으면 
		//값을 초과하게 되서 이상한 결과를 얻게 된다
		res[i] = (res[i - 1] + res[i - 2]) % 15746;
	}

	cout << res[N];

	return 0;
}

