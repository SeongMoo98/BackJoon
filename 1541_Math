#include<iostream>
#include<string>
using namespace std;

string input;


int main(void) {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	cin >> input;
	
	string num;
	int result = 0;
	bool minus = false;

	for (int i = 0; i <= input.size(); i++) {
		if (input[i] == '-' || input[i] == '+' || i == input.size()) {
			//'-'가 한번이라도 나오면 그 뒤의 모든 숫자는 빼준다
			if (minus) {
				result -= stoi(num);
				num = "";
			}
			else {
				result += stoi(num);
				num = "";
			}
		}
		else {
			//substr이 아니라 계속 붙여나감
			num += input[i];
		}
		if (input[i] == '-')
			minus = true;
	}

	cout << result;
	return 0;
}

